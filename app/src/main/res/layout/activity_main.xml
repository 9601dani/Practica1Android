<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.dani96.myapplication1.MainActivity" android:id="@+id/textEnter">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="INGRESE EL TEXTO: "
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintVertical_bias="0.073"
            android:translationX="25sp" android:translationY="50sp"/>
    <Button
            android:text="EJECUTAR"
            android:layout_width="120dp"
            android:layout_height="75dp" android:id="@+id/botonEjecutar" tools:layout_editor_absoluteY="82dp"
            tools:layout_editor_absoluteX="34dp" android:rotationX="2"
            android:layout_alignParentLeft="false" android:translationX="50dp" android:translationY="500sp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextTextPersonName" android:translationY="150sp"/>


</RelativeLayout>